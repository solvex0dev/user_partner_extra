<?xml version="1.0" encoding="UTF-8"?>
<odoo>


    <record model="ir.ui.view" id="view_partner_extra_field">
        <field name="name">res.partner.extra.fields</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="arch" type="xml">
   
            <xpath expr="//field[@name='vat']" position="after">
               
                <field name="partner_nis"></field>
                <field name="partner_nif"></field>
                <field name="partner_ai"></field>
                <field name="partner_rc"></field>
                    
            </xpath>
        </field>
    </record>
</odoo>